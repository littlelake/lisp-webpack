{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    ["env", { "modules": false }]
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}